<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/noTitle"
    android:orientation="vertical"
    android:layout_height="wrap_content"
    android:layout_width="350dip"
    android:layout_centerInParent="true"
    android:background="@android:color/transparent"
    >
    <RelativeLayout 
    android:id="@+id/alert_content"
    android:layout_height="wrap_content"
    android:layout_width="800px"
    android:background="@drawable/tcl_alert_818_content_up"
    > 
	</RelativeLayout>
	 <RelativeLayout 
    android:id="@+id/alert_content_sperate"
    android:layout_height="wrap_content"
    android:layout_width="800px"
    android:background="@drawable/tcl_dialog_sperateline_818"
    > 
	</RelativeLayout>
    <RelativeLayout 
    android:id="@+id/alert_button"
    android:layout_height="90dip"
    android:layout_width="800px"
    android:layout_centerVertical="true"
    android:background="@drawable/tcl_alert_818_foot"
    android:layout_below="@id/alert_content"
    android:layout_alignLeft="@id/alert_content"
    android:layout_alignRight="@id/alert_content"
    >   
<RelativeLayout 
    android:id="@+id/alert_content_sperate"
    android:layout_height="wrap_content"
    android:layout_width="800px"
    android:background="@drawable/tcl_dialog_sperateline_818"
    android:layout_marginTop="20dip"
    /> 
	</RelativeLayout>
</RelativeLayout>
