   	<add-resource type="style" name="TCLProgressBarHW" />
	<style name="TCLProgressBarHW">
		<item name="android:indeterminateOnly">false</item>
		<item name="android:progressDrawable">@android:drawable/tcl_progress_horizontal_white</item>
		<item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
		<item name="android:minHeight">20dip</item>
		<item name="android:maxHeight">20dip</item>
	</style>
   	<add-resource type="style" name="TCLDLabelDialog" />
	<style name="TCLDLabelDialog" parent="android:Theme.Dialog">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFrame">@null</item>
	ã€€      <item name="android:windowIsFloating">true</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:backgroundDimEnabled">false</item>
	</style>
   	<add-resource type="style" name="TCLDLabel" />
	<style name="TCLDLabel" parent="android:Widget.ProgressBar">
		<item name="android:indeterminateDrawable">@android:drawable/tcl_progress_medium</item>
		<item name="android:minWidth">184dip</item>
		<item name="android:maxWidth">184dip</item>
		<item name="android:minHeight">184dip</item>
		<item name="android:maxHeight">184dip</item>
		<item name="android:backgroundDimEnabled">false</item>
	</style>
   	<add-resource type="style" name="TCLSeekBar" />
	<style name="TCLSeekBar">
		<item name="android:indeterminateOnly">false</item>
		<item name="android:progressDrawable">@android:drawable/tcl_seekbar</item>
		<item name="android:indeterminateDrawable">@android:drawable/tcl_seekbar</item>
        <item name="android:minHeight">10px</item>
        <item name="android:maxHeight">10px</item>
        <item name="android:thumb">@android:drawable/tcl_seek_thumb</item>
        <item name="android:paddingLeft">18px</item>
        <item name="android:paddingRight">18px</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_centerInParent">true</item>
	</style>
   	<add-resource type="style" name="TCLRatingBar" />
	<style name="TCLRatingBar">
		<item name="android:indeterminateOnly">false</item>
		<item name="android:progressDrawable">@android:drawable/tcl_ratingbar</item>
		<item name="android:indeterminateDrawable">@android:drawable/tcl_ratingbar</item>
		<item name="android:minHeight">38dip</item>
		<item name="android:maxHeight">38dip</item>
		<item name="android:thumb">@null</item>
		<item name="android:isIndicator">false</item>
	</style>
   	<add-resource type="style" name="TCLEditText" />
	<style name="TCLEditText">
		<item name="android:focusable">true</item>
		<item name="android:focusableInTouchMode">true</item>
		<item name="android:clickable">true</item>
		<item name="android:background">@android:drawable/tcl_edit_text</item>
		<item name="android:textAppearance">?android:attr/textAppearanceMediumInverse</item>
		<!--<item name="android:textColor">@android:color/primary_text_light</item>-->
		<item name="android:textColor">@android:drawable/tcl_btntext_818_selector</item>
		<item name="android:gravity">center_vertical</item>
	</style>
   	<add-resource type="style" name="TCLFloatMenu" />
	<style name="TCLFloatMenu" parent="android:style/Theme.Dialog">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFrame">@null</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:backgroundDimEnabled">false</item>
	</style>
   	<add-resource type="style" name="TCLSenroiMenu" />
	<style name="TCLSenroiMenu" parent="@android:style/Theme.Dialog">
		<item name="android:windowFrame">@null</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:background">@android:color/transparent</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:backgroundDimEnabled">false</item>
		<!--<item name="android:textAppearance">@android:style/tcl_toast_text</item>-->
	</style>
   	<add-resource type="style" name="TCLSpinner" />
	<style name="TCLSpinner">
		<item name="android:background">@android:drawable/tcl_btn_dropdown</item>
		<item name="android:clickable">true</item>
	</style>
   	<add-resource type="style" name="TCLSpinnerDropDownItem" />
	<style name="TCLSpinnerDropDownItem">
		<item name="android:checkMark">@android:drawable/tcl_btn_radio</item>
	</style>
   	<add-resource type="style" name="TCLListView" />
	<style name="TCLListView" parent="Widget.AbsListView">
		<item name="android:listSelector">@android:drawable/tcl_list_selector_818_background</item>
        <item name="android:cacheColorHint">?android:attr/colorBackgroundCacheHint</item>
        <!--item name="android:divider">@android:drawable/screen_background_light</item-->
        <item name="android:divider">@null</item>
	<item name="android:background">@android:drawable/tcl_listview_818_bg</item>
	</style>
   	<add-resource type="style" name="TCLAlertDialog" />
	<style name="TCLAlertDialog" parent="android:style/Theme.Dialog.Alert">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFrame">@null</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:backgroundDimEnabled">false</item>
	</style>
   	<add-resource type="style" name="TCLDialogToast" />
	<style name="TCLDialogToast" parent="android:style/Theme.Dialog.Alert">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFrame">@null</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:backgroundDimEnabled">false</item>
	</style>
   	<add-resource type="style" name="tcl_toast_text" />
	<style name="tcl_toast_text">
		<item name="android:textColor">#ffffff</item>
		<item name="android:textSize">24sp</item>
	</style>
   	<add-resource type="style" name="tcl_alert_text_title" />
	<style name="tcl_alert_text_title">
		<item name="android:textColor">#ffffff</item>
		<item name="android:textSize">30sp</item>
		<item name="android:textStyle">italic</item>
		<item name="android:typeface">monospace</item>
	</style>
   	<add-resource type="style" name="tcl_listdialog_text_title" />
	<style name="tcl_listdialog_text_title">
		<item name="android:textColor">@android:color/primary_text_light</item>
		<item name="android:textSize">30sp</item>
		<item name="android:textStyle">bold</item>
	</style>
