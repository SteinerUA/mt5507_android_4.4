<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- //Begin Added by tianjing@tcl.com for TCL Widgets .22items-->
   	<add-resource type="style" name="TCLButton" />
	<style name="TCLButton">
		<item name="android:background">@android:drawable/tcl_btn_default</item>
		<item name="android:focusable">true</item>
		<item name="android:clickable">true</item>
		<item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <!--item name="android:textColor">@android:color/primary_text_light</item--> 
		<item name="android:textColor">@android:drawable/tcl_btntext_818_selector</item>
		<item name="android:gravity">center_vertical|center_horizontal</item>
	</style>
   	<add-resource type="style" name="TCLButtonBlack" />
	<style name="TCLButtonBlack">
		<item name="android:background">@android:drawable/tcl_btn_black</item>
		<item name="android:focusable">true</item>
		<item name="android:clickable">true</item>
		<item name="android:textAppearance">?android:attr/textAppearanceSmallInverse</item>
        <!--item name="android:textColor">@android:color/primary_text_dark</item-->
		<item name="android:textColor">@android:drawable/tcl_btntext_818_selector</item> 
		<item name="android:gravity">center_vertical|center_horizontal</item>
	</style>
   	<add-resource type="style" name="TCLCheckBox" />
	<style name="TCLCheckBox">
		<item name="android:focusable">true</item>
		<item name="android:clickable">true</item>
		<item name="android:textAppearance">?android:attr/textAppearance</item>
        <!--item name="android:textColor">?android:attr/textColorPrimaryDisableOnly</item-->
        <item name="android:textColor">@android:drawable/tcl_btntext_818_selector</item>
		<item name="android:gravity">center_vertical|left</item>
		<item name="android:background">@android:drawable/tcl_btn_check_label_background</item>
		<item name="android:button">@android:drawable/tcl_btn_check</item>
	</style>
   	<add-resource type="style" name="TCLRadioButton" />
	<style name="TCLRadioButton">
		<item name="android:focusable">true</item>
		<item name="android:clickable">true</item>
		<item name="android:textAppearance">?android:attr/textAppearance</item>
        <!--item name="android:textColor">?android:attr/textColorPrimaryDisableOnly</item-->
        <item name="android:textColor">@android:drawable/tcl_btntext_818_selector</item>
		<item name="android:gravity">center_vertical|left</item>
		<item name="android:background">@android:drawable/tcl_btn_radio_label_background</item>
		<item name="android:button">@android:drawable/tcl_btn_radio</item>
	</style>
   	<add-resource type="style" name="TCLProgressBarH" />
	<style name="TCLProgressBarH">
		<item name="android:indeterminateOnly">false</item>
		<item name="android:progressDrawable">@android:drawable/tcl_progress_horizontal</item>
		<item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
		<item name="android:minHeight">20dip</item>
		<item name="android:maxHeight">20dip</item>
	</style>
