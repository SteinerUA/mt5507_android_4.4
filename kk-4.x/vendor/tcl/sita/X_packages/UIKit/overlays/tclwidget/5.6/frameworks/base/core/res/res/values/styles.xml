		<item name="android:indeterminateOnly">true</item>
		<item name="android:indeterminateDrawable">@android:drawable/tcl_dlabel</item>
		<item name="android:minWidth">160dip</item>
        	<item name="android:maxWidth">160dip</item>
        	<item name="android:minHeight">146dip</item>
        	<item name="android:maxHeight">146dip</item>
	</style>
