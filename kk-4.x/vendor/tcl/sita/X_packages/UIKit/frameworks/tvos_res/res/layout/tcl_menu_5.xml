<?xml version="1.0" encoding="utf-8"?>
<LinearLayout  xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tvos="http://schemas.android.com/apk/res/tvos" 
	android:orientation="horizontal" 
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent"
	android:background="#00FFFFFF"
	
	
	>
	<!--RelativeLayout 
		android:id="@+id/tcl_floatmenu_4"	
		android:orientation="horizontal"
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent"
		android:layout_marginTop="60px"
		android:layout_gravity="center"
		android:gravity="center_horizontal"
		android:background="#00FFFFFF"
				
		-->
		<RelativeLayout 		
			android:id="@+tvos:id/left_RelativeLayout"
			android:layout_width="200dp" 
			android:layout_height="145dp"			
			android:background="@tvos:drawable/tcl_menu_left"
			android:focusable="true"
			android:clickable="true"
			android:orientation="vertical"
			android:layout_weight="1">
			<RelativeLayout
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_centerInParent="true"	
			>
			<ImageView
			android:id="@+tvos:id/left_icon"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:src="@tvos:drawable/tcl_icon_wallpaper"
			android:focusable="true"		
			android:layout_centerHorizontal="true"		
			/>
			<TextView
			android:id="@+tvos:id/left_text"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:text="@tvos:string/tcl_menu_wallpaper"
			android:focusable="true"
			android:layout_below="@tvos:id/left_icon"	
			android:layout_marginBottom="6dp"	
			android:layout_weight="2"	
			android:layout_centerHorizontal="true"
			android:textAppearance="@tvos:style/tcl_toast_text"		
			/>
		
			</RelativeLayout>
		</RelativeLayout>
		
		<RelativeLayout 
		    android:id="@+tvos:id/separateline_RelativeLayout"
		    android:layout_width="0.5dp"
		    android:layout_height="145dp"
		    android:background="@tvos:drawable/tcl_separateline2"
			android:layout_toRightOf="@tvos:id/left_RelativeLayout"
			android:layout_alignTop="@tvos:id/left_RelativeLayout"
			android:layout_alignBottom="@tvos:id/left_RelativeLayout"
		    >
		</RelativeLayout> 
		
		<RelativeLayout 		
			android:id="@+tvosid/left_RelativeLayout2"
			android:layout_width="200dp" 
			android:layout_height="145dp"			
			android:background="@tvos:drawable/tcl_menu_mid"
			android:focusable="true"
			android:clickable="true"
			android:layout_toRightOf="@tvos:id/separateline_RelativeLayout"
			android:orientation="vertical"
			android:layout_weight="1">
			<RelativeLayout
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_centerInParent="true"	
			>
			<ImageView
			android:id="@+id/left_icon2"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:src="@tvos:drawable/tcl_icon_move"
			android:focusable="true"		
			android:layout_centerHorizontal="true"		
			/>
			<TextView
			android:id="@+tvos:id/left_text2"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:text="@tvos:string/tcl_menu_move"
			android:focusable="true"
			android:layout_below="@tvos:id/left_icon2"	
			android:layout_marginBottom="6dp"	
			
			android:layout_centerHorizontal="true"
			android:textAppearance="@tvos:style/tcl_toast_text"		
			/>
		
			</RelativeLayout>
		</RelativeLayout>
		
		<RelativeLayout 
		    android:id="@+tvos:id/separateline_RelativeLayout1"
		    android:layout_width="0.5dp"
		    android:layout_height="145dp"
		    android:background="@tvos:drawable/tcl_separateline2"
			android:layout_toRightOf="@tvos:id/left_RelativeLayout2"
			android:layout_alignTop="@tvos:id/left_RelativeLayout2"
			android:layout_alignBottom="@tvos:id/left_RelativeLayout2"
		    >
		</RelativeLayout> 
		
		<RelativeLayout 		
			android:id="@+tvos:id/mid_RelativeLayout"
			android:layout_width="200dp" 
			android:layout_height="145dp"			
			android:background="@tvos:drawable/tcl_menu_mid"
			android:focusable="true"
			android:clickable="true"
			android:layout_toRightOf="@tvos:id/separateline_RelativeLayout1"
			android:orientation="vertical"
			android:layout_weight="1">
			<RelativeLayout
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_centerInParent="true"	
			>
			<ImageView
			android:id="@+id/mid_icon"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:src="@tvos:drawable/tcl_icon_delete"
			android:focusable="true"		
			android:layout_centerHorizontal="true"		
			/>
			<TextView
			android:id="@+tvos:id/mid_text"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:text="@tvos:string/tcl_menu_delete"
			android:textAppearance="@tvos:style/tcl_toast_text"
			android:focusable="true"
			android:layout_below="@tvos:id/mid_icon"	
			android:layout_marginBottom="10dp"		
			android:layout_centerHorizontal="true"
		
			/>
		
			</RelativeLayout>
		</RelativeLayout>	
		
		<RelativeLayout 
		    android:id="@+tvos:id/separateline_RelativeLayout2"
		    android:layout_width="0.5dp"
		    android:layout_height="145dp"
		    android:background="@tvos:drawable/tcl_separateline2"
			android:layout_toRightOf="@tvos:id/mid_RelativeLayout"
			android:layout_alignTop="@tvos:id/mid_RelativeLayout"
			android:layout_alignBottom="@tvos:id/mid_RelativeLayout"
		    >
		</RelativeLayout>
		
		<RelativeLayout 
			android:id="@+tvos:id/right_RelativeLayout2"
			android:layout_width="200dp" 
			android:layout_height="145dp"
			android:background="@tvos:drawable/tcl_menu_mid"
			
			android:focusable="true"
			android:clickable="true"
			android:orientation="vertical"
			android:layout_toRightOf="@tvos:id/separateline_RelativeLayout2"
			android:layout_weight="1"	
			>
			<RelativeLayout
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="#00FFFFFF"
			>
			<ImageView
			android:id="@+tvos:id/right_icon2"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:src="@tvos:drawable/tcl_icon_add"
			android:focusable="true"
			android:layout_centerHorizontal="true"
			/>
			<TextView
			android:id="@+tvos:id/right_text2"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:text="@string/tcl_menu_add"
			android:textAppearance="@tvos:style/tcl_toast_text"
			android:focusable="true"
			android:layout_below="@tvos:id/right_icon2"	
			android:layout_marginBottom="10dp"		
			android:layout_centerHorizontal="true"		
			/>
			</RelativeLayout>
		</RelativeLayout>
		
		<RelativeLayout 
		    android:id="@+tvos:id/separateline_RelativeLayout3"
		    android:layout_width="0.5dp"
		    android:layout_height="145dp"
		    android:background="@tvos:drawable/tcl_separateline2"
			android:layout_toRightOf="@tvos:id/right_RelativeLayout2"
			android:layout_alignTop="@tvos:id/right_RelativeLayout2"
			android:layout_alignBottom="@tvos:id/right_RelativeLayout2"
		    >
		</RelativeLayout>
		
		<RelativeLayout 
			android:id="@+tvos:id/right_RelativeLayout"
			android:layout_width="200dp" 
			android:layout_height="145dp"
			android:background="@tvos:drawable/tcl_menu_right"
			
			android:focusable="true"
			android:clickable="true"
			android:orientation="vertical"
			android:layout_toRightOf="@tvos:id/separateline_RelativeLayout3"
			android:layout_weight="1"	
			>
			<RelativeLayout
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:background="#00FFFFFF"
			>
			<ImageView
			android:id="@+tvos:id/right_icon"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:src="@tvos:drawable/tcl_icon_quit"
			android:focusable="true"
			android:layout_centerHorizontal="true"
			/>
			<TextView
			android:id="@+tvos:id/right_text"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:text="@tvos:string/tcl_menu_quit"
			android:textAppearance="@tvos:style/tcl_toast_text"
			android:focusable="true"
			android:layout_below="@tvos:id/right_icon"	
			android:layout_marginBottom="10dp"		
			android:layout_centerHorizontal="true"		
			/>
			</RelativeLayout>
		</RelativeLayout>
		
		<!--/RelativeLayout-->
		
</LinearLayout >

